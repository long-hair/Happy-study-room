page{
  width: 100%;
  height: 100%;
}
.changeTime{
  position: relative;
  width: 100%;
  height: 100%;
}
.changeTime .top{
  height: 74rpx;
  line-height: 74rpx;
  padding: 46rpx 20rpx 0;
  font-size: 26rpx;
  color: #666;
  display: flex;
  align-items: center;
}
.changeTime .bot{
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  background-color: #9291D4;
  font-size: 30rpx;
  color: #fff;
}